package quiz22;

import java.util.Arrays;
import java.util.List;
import java.util.regex.Matcher;
import java.util.regex.Pattern;

public class Productex {
	
	public static void main(String[] args) {
		
		String s = "20180428-01-024720180428 건담베이스 강남점 [RG] 건담베이스 한정판 시난주 (메탈릭 글로스 인젝션) 48000원";

		//String pattern1 = "\\d{8}-\\d{2}-\\d+";
		//String pattern2 = "건담[\\s가-힣]*";
		String pattern3 = "\\[[가-힣A-Z]*\\]";
		String pattern5 = "[0-9,]+원";

		
		Matcher i = Pattern.compile(pattern3).matcher(s);
		Matcher p = Pattern.compile(pattern5).matcher(s);
		while(i.find()&& p.find()) {
		System.out.println(i.end()+ " "+ p.start());
		s = s.substring(i.end(), p.start());
		System.out.println(s);
		}
		
		
		
		
		
	}

}
